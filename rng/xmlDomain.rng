     Copyright (c) 2009, 2010 DITA For Publishers
     
     This domain module may be used by anyone without restriction.
     
     =============================================================
</headerComment>
      <moduleMetadata>
         <moduleType>elementdomain</moduleType>
         <moduleShortName>xml-d</moduleShortName>
         <modulePublicIds>
            <dtdMod>urn:xxx:dtd:xml.mod</dtdMod>
            <dtdEnt>urn:xxx:dtd:xml.ent</dtdEnt>
            <xsdMod>urn:xxx:xsd:xml.xsd</xsdMod>
            <rncMod>urn:xxx:rnc:xml.rnc</rncMod>
            <rngMod>urn:xxx:rng:xml.rng</rngMod>
         </modulePublicIds>
         <domainsContribution>(topic xml-d)</domainsContribution>
      </moduleMetadata>
   </moduleDesc>
   <div>
      <a:documentation>DOMAIN EXTENSION PATTERNS</a:documentation>
      <define name="xml-d-xxx">
         <ref name="xmlelem.element"/>
         <ref name="xmlatt.element"/>
         <ref name="textent.element"/>
         <ref name="parment.element"/>
      </define>
   </div>
   <div>
      <a:documentation>ELEMENT TYPE NAME PATTERNS</a:documentation>
      <define name="xmlelem">
         <ref name="xmlelem.element"/>
      </define>
      <define name="xmlatt">
         <ref name="xmlatt.element"/>
      </define>
      <define name="textent">
         <ref name="textent.element"/>
      </define>
      <define name="parment">
         <ref name="parment.element"/>
      </define>
   </div>
   <div>
      <a:documentation>ELEMENT TYPE DECLARATIONS</a:documentation>
      <div>
         <a:documentation>Long Name: xmlelem</a:documentation>
         <define name="xmlelem.content">
            <zeroOrMore>
               <choice>
                  <rng:text/>
                  <rng:ref name="keyword"/>
                  <rng:ref name="text"/>
               </choice>
            </zeroOrMore>
         </define>
         <define name="xmlelem.attributes">
            <ref name="univ-atts"/>
            <optional>
               <attribute name="keyref"/>
            </optional>
            <optional>
               <attribute name="outputclass"/>
            </optional>
         </define>
         <define name="xmlelem.element">
            <element name="xmlelem" dita:longName="xmlelem">
               <a:documentation>docs go here.</a:documentation>
               <ref name="xmlelem.attlist"/>
               <ref name="xmlelem.content"/>
            </element>
         </define>
         <define name="xmlelem.attlist" combine="interleave">
            <ref name="xmlelem.attributes"/>
         </define>
      </div>
