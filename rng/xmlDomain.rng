   <div>
      <a:documentation>Specialization attributes. Global attributes and class defaults</a:documentation>
      <define name="xmlelem.attlist" combine="interleave">
         <ref name="global-atts"/>
         <optional>
            <attribute name="class" a:defaultValue="+ topic/keyword xml-d/xmlelem "/>
